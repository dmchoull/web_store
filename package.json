{
  "name": "TBD",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "preexport": "yarn build",
    "export": "next export",
    "serve": "serve out --listen 8080",
    "deploy": "now --public -e BB_API_KEY=@bb-api-key",
    "test": "jest",
    "test:watch": "jest --watch --notify",
    "lint": "eslint pages __mocks__ cypress --ext .js --ext .jsx"
  },
  "browserslist": [
    "> 1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn run lint-staged",
      "pre-push": "yarn test && ./e2e.sh",
      "post-merge": "yarnhook",
      "post-checkout": "yarnhook",
      "post-rewrite": "yarnhook"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add",
      "jest --findRelatedTests"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "airbnb-prop-types": "^2.9.0",
    "axios": "^0.18.0",
    "bestbuy": "^2.1.3",
    "next": "^6.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "cypress": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "dom-testing-library": "^3.0.0",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.4.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^1.0.0-rc.9",
    "jest": "^23.2.0",
    "jest-runner-eslint": "^0.6.0",
    "jest-watch-typeahead": "^0.2.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "react-testing-library": "^4.1.0",
    "serve": "^9.1.0",
    "yarnhook": "^0.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
