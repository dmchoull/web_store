{
  "name": "TBD",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "preexport": "yarn build",
    "export": "next export",
    "serve": "serve out --listen 8080",
    "deploy": "now --public -e BB_API_KEY=@bb-api-key",
    "test": "jest",
    "test:watch": "jest --watch --notify",
    "lint": "eslint pages __mocks__ cypress --ext .js --ext .jsx"
  },
  "browserslist": [
    "> 1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint-staged",
      "pre-push": "yarn test && ./e2e.sh",
      "post-merge": "yarnhook",
      "post-checkout": "yarnhook",
      "post-rewrite": "yarnhook"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add",
      "jest --findRelatedTests"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "airbnb-prop-types": "^2.9.0",
    "axios": "^0.18.0",
    "bestbuy": "^2.1.3",
    "next": "^6.0.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.47",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "cypress": "^3.0.1",
    "dom-testing-library": "^2.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.3.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^1.0.0-rc.8",
    "jest": "^23.1.0",
    "jest-runner-eslint": "^0.6.0",
    "jest-watch-typeahead": "^0.1.0",
    "lint-staged": "^7.1.3",
    "prettier": "^1.13.4",
    "react-testing-library": "^3.0.2",
    "serve": "^7.2.0",
    "yarnhook": "^0.2.0"
  }
}
